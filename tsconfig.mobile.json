{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "noImplicitAny": false,
    "exactOptionalPropertyTypes": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "App.tsx",
    "index.ts"
  ],
  "exclude": [
    "node_modules",
    "functions",
    "speaksync-web",
    "e2e",
    "**/__tests__/**"
  ]
}
